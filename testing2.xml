def GetLSProData(page_data,Cookie_Jar,m):
	import requests
	import re
	import base64
	source = page_data
	room = re.findall('roomSlug\": \"(.*?)\"', source)[0]
	url = 'https://lancer.streamup.com/api/redirect/' + room
	referer = 'https://streamup.com/'
	origin = 'https://streamup.com'
